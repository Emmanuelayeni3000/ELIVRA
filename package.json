{
  "name": "wedvite",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:generate": "npx dotenv-cli -e ../.env.development.local -- npx prisma generate",
    "db:push": "npx dotenv-cli -e ../.env.development.local -- npx prisma db push",
    "db:migrate": "npx dotenv-cli -e ../.env.development.local -- npx prisma migrate dev",
    "db:studio": "npx prisma studio",
    "db:seed": "npx dotenv-cli -e ../.env.development.local -- npx prisma db seed",
    "db:reset": "npx dotenv-cli -e ../.env.development.local -- npx prisma migrate reset",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "ANALYZE=true npm run build"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@hookform/resolvers": "^5.2.1",
